{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
            "@/*": ["./*"]
        }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/layout.tsx", "app/page.jsx", "app/about/page.jsx", "app/contact/page.jsx", "app/blog/page.jsx", "app/blog/layout.jsx", "app/blog/latihan-route-next/page.jsx", "app/blog/judul-berita/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
